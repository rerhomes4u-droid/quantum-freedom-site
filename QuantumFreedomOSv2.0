            flex-wrap: wrap;
            gap: 10px;
            margin: 2rem 0;
        }

        .bead {
            width: 30px;
            height: 30px;
            border-radius: 50%;
            background: var(--spiritual-gold);
            display: flex;
            align-items: center;
            justify-content: center;
            cursor: pointer;
            transition: all 0.3s ease;
            color: var(--dark-text);
            font-weight: bold;
        }

        .bead:hover {
            transform: scale(1.2);
            box-shadow: 0 0 15px var(--spiritual-gold);
        }

        .bead.active {
            background: var(--colombia-red);
            color: white;
        }

        .rosary-controls {
            display: flex;
            justify-content: center;
            gap: 1rem;
            margin-top: 2rem;
        }

        .rosary-prayer-display {
            background: rgba(255, 255, 255, 0.05);
            border-radius: 15px;
            padding: 2rem;
            margin: 2rem 0;
            min-height: 200px;
            font-family: 'Times New Roman', serif;
            font-size: 1.2rem;
            line-height: 1.8;
        }

        /* Spanish Content */
        .spanish-content {
            display: none;
        }
    </style>

    <title>Quantum Freedom OS v2.0 - Sovereign System</title>
    <style>
        :root {
            --colombia-yellow: #FCD116;
            --colombia-blue: #003893;
            --colombia-red: #CE1126;
            --white: #FFFFFF;
            --dark-text: #333333;
            --light-gray: #f5f5fa;
            --success-green: #28a745;
            --electric-blue: #0066ff;
            --quantum-purple: #8b5cf6;
            --spiritual-gold: #FFD700;
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }

        body {
            background: linear-gradient(135deg, #0f0f23 0%, #1a1a2e 50%, #16213e 100%);
            color: var(--white);
            line-height: 1.6;
            min-height: 100vh;
            transition: all 0.5s ease;
        }

        /* Quantum Header */
        .quantum-header {
            background: linear-gradient(135deg, 
                var(--colombia-yellow) 0%, 
                var(--electric-blue) 50%, 
                var(--quantum-purple) 100%);
            padding: 1rem 0;
            position: relative;
            overflow: hidden;
        }

        .quantum-header::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: 
                radial-gradient(circle at 20% 80%, rgba(252, 209, 22, 0.3) 0%, transparent 50%),
                radial-gradient(circle at 80% 20%, rgba(0, 102, 255, 0.3) 0%, transparent 50%),
                radial-gradient(circle at 40% 40%, rgba(139, 92, 246, 0.3) 0%, transparent 50%);
            animation: quantumPulse 8s ease-in-out infinite;
        }

        @keyframes quantumPulse {
            0%, 100% { opacity: 0.5; transform: scale(1); }
            50% { opacity: 0.8; transform: scale(1.02); }
        }

        .header-container {
            max-width: 1400px;
            margin: 0 auto;
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 0 2rem;
            position: relative;
            z-index: 2;
        }

        .logo {
            display: flex;
            align-items: center;
            gap: 15px;
        }

        .logo-text {
            font-size: 2rem;
            font-weight: 800;
            color: var(--white);
            text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);
        }

        .stars {
            color: var(--white);
            font-size: 1.2rem;
            letter-spacing: 2px;
        }

        .nav-pills {
            display: flex;
            gap: 1rem;
        }

        .nav-pill {
            background: rgba(255, 255, 255, 0.1);
            color: var(--white);
            padding: 0.7rem 1.5rem;
            border-radius: 25px;
            text-decoration: none;
            font-weight: 600;
            transition: all 0.3s ease;
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255, 255, 255, 0.2);
        }

        .nav-pill:hover {
            background: rgba(255, 255, 255, 0.2);
            transform: translateY(-2px);
        }

        /* Main Container */
        .container {
            max-width: 1400px;
            margin: 0 auto;
            padding: 2rem;
        }

        /* Quantum Hero */
        .quantum-hero {
            text-align: center;
            padding: 4rem 0;
            margin-bottom: 2rem;
            position: relative;
        }

        .quantum-hero::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: 
                radial-gradient(circle at 50% 50%, rgba(252, 209, 22, 0.1) 0%, transparent 50%),
                radial-gradient(circle at 80% 20%, rgba(0, 102, 255, 0.1) 0%, transparent 50%);
            z-index: -1;
        }

        .hero-badge {
            display: inline-block;
            background: linear-gradient(135deg, var(--colombia-yellow), var(--electric-blue));
            color: var(--dark-text);
            padding: 1rem 2rem;
            border-radius: 50px;
            font-weight: 800;
            margin-bottom: 2rem;
            box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3);
            animation: float 6s ease-in-out infinite;
        }

        @keyframes float {
            0%, 100% { transform: translateY(0px); }
            50% { transform: translateY(-10px); }
        }

        .quantum-hero h1 {
            font-size: 4rem;
            background: linear-gradient(135deg, var(--colombia-yellow), var(--electric-blue), var(--quantum-purple));
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            margin-bottom: 1.5rem;
            text-shadow: 0 0 30px rgba(252, 209, 22, 0.3);
        }

        .hero-subtitle {
            font-size: 1.5rem;
            color: rgba(255, 255, 255, 0.8);
            max-width: 800px;
            margin: 0 auto 3rem;
            line-height: 1.8;
        }

        /* Quantum Grid System */
        .quantum-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(400px, 1fr));
            gap: 2rem;
            margin: 3rem 0;
        }

        .quantum-card {
            background: rgba(255, 255, 255, 0.05);
            backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 20px;
            padding: 2.5rem;
            transition: all 0.3s ease;
            position: relative;
            overflow: hidden;
        }

        .quantum-card::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 3px;
            background: linear-gradient(90deg, var(--colombia-yellow), var(--electric-blue), var(--quantum-purple));
        }

        .quantum-card:hover {
            transform: translateY(-10px);
            box-shadow: 
                0 20px 40px rgba(0, 0, 0, 0.3),
                0 0 80px rgba(252, 209, 22, 0.1);
        }

        .card-icon {
            font-size: 3.5rem;
            margin-bottom: 1.5rem;
            display: block;
        }

        .card-badge {
            position: absolute;
            top: 1rem;
            right: 1rem;
            background: var(--success-green);
            color: white;
            padding: 0.3rem 1rem;
            border-radius: 15px;
            font-size: 0.8rem;
            font-weight: 700;
        }

        /* Tool Container */
        .tool-container {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.9);
            z-index: 10000;
            justify-content: center;
            align-items: center;
            backdrop-filter: blur(10px);
        }

        .quantum-tool {
            background: rgba(30, 30, 46, 0.95);
            backdrop-filter: blur(30px);
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 25px;
            width: 95%;
            max-width: 1200px;
            max-height: 95vh;
            overflow: hidden;
            box-shadow: 
                0 25px 50px rgba(0, 0, 0, 0.5),
                0 0 100px rgba(252, 209, 22, 0.2);
        }

        .tool-header {
            background: linear-gradient(135deg, var(--colombia-yellow), var(--electric-blue));
            color: white;
            padding: 2rem;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .close-tool {
            background: none;
            border: none;
            color: white;
            font-size: 2rem;
            cursor: pointer;
            padding: 0.5rem;
            transition: transform 0.3s ease;
        }

        .close-tool:hover {
            transform: scale(1.2);
        }

        .tool-content {
            padding: 2rem;
            max-height: 70vh;
            overflow-y: auto;
        }

        /* Form Styles */
        .form-group {
            margin-bottom: 2rem;
        }

        .form-group label {
            display: block;
            margin-bottom: 0.8rem;
            font-weight: 600;
            color: var(--colombia-yellow);
            font-size: 1.1rem;
        }

        .quantum-input {
            width: 100%;
            padding: 1rem;
            background: rgba(255, 255, 255, 0.05);
            border: 2px solid rgba(255, 255, 255, 0.1);
            border-radius: 12px;
            font-size: 1rem;
            color: white;
            transition: all 0.3s ease;
        }

        .quantum-input:focus {
            outline: none;
            border-color: var(--electric-blue);
            box-shadow: 0 0 20px rgba(0, 102, 255, 0.3);
        }

        .quantum-btn {
            background: linear-gradient(135deg, var(--colombia-yellow), var(--electric-blue));
            color: white;
            border: none;
            padding: 1.2rem 2.5rem;
            border-radius: 12px;
            font-size: 1.1rem;
            font-weight: 700;
            cursor: pointer;
            transition: all 0.3s ease;
            text-decoration: none;
            display: inline-block;
            text-align: center;
            box-shadow: 0 8px 25px rgba(0, 0, 0, 0.3);
        }

        .quantum-btn:hover {
            transform: translateY(-3px);
            box-shadow: 0 12px 35px rgba(0, 0, 0, 0.4);
            background: linear-gradient(135deg, var(--electric-blue), var(--quantum-purple));
        }

        /* Results */
        .quantum-results {
            background: rgba(255, 255, 255, 0.05);
            border-radius: 15px;
            padding: 2rem;
            margin-top: 2rem;
            border-left: 4px solid var(--colombia-yellow);
            animation: slideInUp 0.6s ease;
        }

        @keyframes slideInUp {
            from {
                opacity: 0;
                transform: translateY(30px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }


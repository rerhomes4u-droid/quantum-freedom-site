        }

        @keyframes fadeIn {
            from { opacity: 0; transform: translateY(20px); }
            to { opacity: 1; transform: translateY(0); }
        }

        .spiritual-header {
            text-align: center;
            margin-bottom: 3rem;
        }

        .spiritual-title {
            font-size: 3rem;
            background: linear-gradient(135deg, var(--spiritual-gold), var(--colombia-yellow));
